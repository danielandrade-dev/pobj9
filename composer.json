{
    "name": "pobj/api",
    "description": "API PHP para o painel POBJ",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^3.7",
        "doctrine/migrations": "^3.9",
        "doctrine/orm": "^2.16",
        "symfony/cache": "^6.0|^7.0",
        "symfony/var-dumper": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Pobj\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pobj\\Api\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
